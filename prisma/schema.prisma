generator client {
  provider = "prisma-client-js"
}

generator graphql {
  provider = "graphql-schema-generator"
  createCRUD = "true"
  output = "./generated"
}
datasource db {
  provider = "mysql"
  url      = env("DATABASE_URL")
}

model Task {
  id          Int     @id @default(autoincrement())
  title       String
  description String?
}
